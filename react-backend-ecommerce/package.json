{
  "type": "module",
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "React Express App",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "forever start -w server.js",
    "forever-list": "forever list",
    "forever-stop-id": "forever stop",
    "forever-stop-all": "forever stopall",
    "forever-help": "forever --help",
    "pm2": "pm2 start server.js"
  },
  "author": "Lucas Cueva",
  "license": "MIT",
  "devDependencies": {
    "@types/pino": "^6.3.11",
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "artillery": "^1.7.6",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "concurrently": "^3.5.1",
    "connect-mongo": "^4.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "faker": "^5.5.3",
    "firebase-admin": "^9.9.0",
    "graphql": "^15.5.1",
    "knex": "^0.95.6",
    "moment": "^2.29.1",
    "mongoose": "^5.13.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.6.3",
    "normalizr": "^3.6.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "pino": "^6.13.0",
    "pino-pretty": "^6.0.0",
    "pm2": "^5.1.0",
    "react-router-dom": "^5.2.0",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "sqlite3": "^5.0.2",
    "twilio": "^3.66.1"
  }
}
